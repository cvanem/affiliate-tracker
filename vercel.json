{
    "build": {
      "env": {
        "DATABASE_URL": "@database_url",
        "BEARER_TOKEN": "@bearer_token"
      }
    },
    "functions": {
      "api/edge.js": {
        "runtime": "@vercel/node@3.0.5"
      }
    },
    "routes": [
      {
        "src": "/booking",
        "dest": "api/edge.js"
      },
      {
        "src": "/webhook", 
        "dest": "api/calendlyWebhook.js"
      }
    ]
  }
  